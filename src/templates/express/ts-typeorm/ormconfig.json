{
    "type": "postgres",
    "name": "default",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "ts_typeorm_api",
    "synchronize": true,
    "logging": false,
    "entities": [
        "dist/api/core/models/**/*.model.js",
        "dist/api/resources/**/*.model.js"
    ],
    "migrations": [
        "dist/migrations/**/*.js"
    ],
    "subscribers": [
        "dist/api/core/subscribers/**/*.subscriber.js",
        "dist/api/resources/**/*.subscriber.js"
    ],
    "cli": {
        "migrationsDir": "./dist/migrations"
    }
}