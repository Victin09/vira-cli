{
    "version": "1.0.0",
    "engines": {
        "node": ">=14.16",
        "npm": ">=6.14.11"
    },
    "author": "Viti Gomez",
    "license": "MIT",
    "name": "ts-typeorm-api-boilerplate",
    "description": "REST API boilerplate builded with Typescript, Express.js, TypeORM and Mocha.",
    "keywords": [
        "node",
        "express.js",
        "typescript",
        "typeorm",
        "mysql",
        "boilerplate",
        "api",
        "rest",
        "restful",
        "es2017",
        "mocha"
    ],
    "homepage": "https://github.com/Victin09/vira-cli/tree/master/src/templates/express/ts-typeorm#README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Victin09/vira-cli.git"
    },
    "bugs": {
        "url": "https://github.com/Victin09/vira-cli/issues"
    },
    "main": "./dist/api/app.bootstrap.js",
    "auto-changelog": {
        "output": "CHANGELOG.md",
        "template": "keepachangelog",
        "unreleased": false,
        "commitLimit": false
    },
    "scripts": {
        "lint": "eslint ./src",
        "lint:fix": "eslint --fix ./src",
        "lint:fix-all": "eslint --fix ./",
        "init": "npm-run-all -s init:*",
        "init:env": "cp ./src/env/template.env ./src/env/development.env && cp ./src/env/template.env ./src/env/test.env && cp ./src/env/template.env ./src/env/staging.env && cp ./src/env/template.env ./src/env/production.env",
        "ci:test": "nyc --reporter=lcov --report-dir=./reports/coverage npm-run-all -s test:*",
        "doc": "apidoc -i src/ -o docs/apidoc/",
        "start": "rm -r ./dist/ && mkdir -p ./dist/env && npm run kickstart:dev",
        "kickstart:dev": "cp ./src/env/development.env ./dist/env/development.env  && cp ./src/env/test.env ./dist/env/test.env && tsc && nodemon -q -w dist dist/app.bootstrap.js",
        "kickstart:staging": "npm run kickstart && cp ./src/env/staging.env ./dist/env/staging.env && tsc",
        "kickstart:production": "npm run kickstart && cp ./src/env/production.env ./dist/env/production.env && tsc",
        "kickstart:test": "npm run kickstart && cp ./src/env/test.env ./dist/env/test.env && tsc",
        "test": "nyc --reporter=html --report-dir=./reports/nyc-coverage npm-run-all -s test:*",
        "test:unit": "./node_modules/.bin/mocha ./test/units/00-application.unit.test.js --exit --reporter spec --timeout 10000 --env test",
        "test:e2e": "typeorm schema:drop && typeorm schema:sync && ./node_modules/.bin/mocha ./test/e2e/00-api.e2e.test.js --exit --reporter spec --timeout 10000 --env test",
        "todo": "leasot -x --reporter markdown './src/**/*.ts' > TODO.md",
        "prettier": "prettier --write \"./src/**/*.{js,ts,json}\"",
        "version": "npm run todo && auto-changelog -p"
    },
    "_moduleAliases": {
        "@core": "dist/api/core",
        "@classes": "dist/api/core/types/classes",
        "@config": "dist/api/config",
        "@controllers": "dist/api/core/controllers",
        "@decorators": "dist/api/core/types/decorators",
        "@enums": "dist/api/core/types/enums",
        "@errors": "dist/api/core/types/errors",
        "@events": "dist/api/core/types/events",
        "@factories": "dist/api/core/factories",
        "@interfaces": "dist/api/core/types/interfaces",
        "@middlewares": "dist/api/core/middlewares",
        "@models": "dist/api/core/models",
        "@repositories": "dist/api/core/repositories",
        "@resources": "dist/api/resources",
        "@routes": "dist/api/core/routes/v1",
        "@schemas": "dist/api/core/types/schemas",
        "@services": "dist/api/core/services",
        "@types": "dist/api/core/types/types",
        "@utils": "dist/api/core/utils",
        "@validations": "dist/api/core/validations"
    },
    "dependencies": {
        "@hapi/boom": "9.1.1",
        "axios": "0.21.1",
        "bcryptjs": "^2.4.3",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dayjs": "^1.10.4",
        "dotenv": "8.2.0",
        "es6-promisify": "6.1.1",
        "express": "4.16.4",
        "express-rate-limit": "5.2.6",
        "filenamify": "4.2.0",
        "helmet": "4.4.1",
        "hpp": "0.2.3",
        "http-status": "1.5.0",
        "joi": "17.4.0",
        "jwt-simple": "0.5.6",
        "lodash": "4.17.21",
        "memory-cache": "0.2.0",
        "module-alias": "2.2.2",
        "morgan": "1.10.0",
        "passport": "0.4.1",
        "passport-facebook": "3.0.0",
        "passport-github2": "0.1.12",
        "passport-google-oauth20": "^2.0.0",
        "passport-http-bearer": "1.0.1",
        "passport-jwt": "4.0.0",
        "passport-linkedin-oauth2": "2.0.0",
        "pg": "^8.7.1",
        "pluralize": "8.0.0",
        "reflect-metadata": "0.1.13",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.34",
        "uuid": "8.3.2",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "1.19.0",
        "@types/compression": "^1.7.1",
        "@types/cors": "^2.8.12",
        "@types/es6-promisify": "^6.0.0",
        "@types/express": "4.16.0",
        "@types/express-rate-limit": "^5.1.3",
        "@types/hpp": "^0.2.2",
        "@types/lodash": "^4.14.171",
        "@types/morgan": "^1.9.3",
        "@types/node": "12.7.2",
        "@types/passport": "^1.0.7",
        "@types/passport-facebook": "^2.1.11",
        "@types/passport-github2": "^1.2.5",
        "@types/passport-google-oauth20": "^2.0.9",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-linkedin-oauth2": "^1.5.3",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "4.18.0",
        "@typescript-eslint/parser": "4.18.0",
        "apidoc": "^0.27.0",
        "auto-changelog": "2.2.1",
        "chai": "4.3.4",
        "chance": "1.1.7",
        "coveralls": "3.1.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "32.2.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "^3.4.0",
        "leasot": "11.5.0",
        "mocha": "8.3.0",
        "nock": "10.0.6",
        "node-notifier": "8.0.1",
        "nodemon": "2.0.7",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "prettier": "^2.3.2",
        "rsgen": "^0.3.2",
        "sinon": "9.2.4",
        "supertest": "6.1.3",
        "typescript": "4.2.2",
        "typescript-eslint": "0.0.1-alpha.0"
    }
}
